{
  "name": "webmusicplayer",
  "version": "0.1.0",
  "main": "index.html",
  "single-instance":true,
  "inject_js_start": "./js/init.js",
  "window": {
    "title": "webmusicplayer",
    "resizable": true,
    "show_in_taskbar": true,
    "min_width": 1024,
    "min_height": 768
  },
  "webkit": {
    "plugin": true
  },
  "chromium-args": "--load-plugin=ffmpegsumo.dll --disable-direct-composition --allow-running-insecure-content",
  "node-remote": "*://*"
  }

